{
    "manifest_version": 3,
    "name": "Jump To",
    "description": "Browse without your mouse",
    "version": "1.0",
    "action": {
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    },
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
  }